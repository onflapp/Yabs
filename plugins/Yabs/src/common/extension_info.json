{
    "background_scripts": [
        "main.js"
    ],
    "browser_button": {
        "caption": "Yabs",
        "icon": "icons/button.png", 
        "tooltipText": "turn sync on and off"
    },
    "content_scripts": [
        "content.js"
    ],
    "creator": "OnFlApp",
    "description": "Yabs synchronized browser content remotely",
    "homepage_url": "https://github.com/onflapp/Yabs",
    "name": "Yabs",
    "permissions": {
        "content_scripts": [
            "http://*/*",
						"<all_urls>",
            "https://*/*",
            "file://*/*"
        ],
        "context_menu": true,
        "cookies": false,
        "notifications": false,
        "tabs": true,
        "web_navigation": true,
        "xhr": [
            "http://*/*",
						"<all_urls>",
            "https://*/*",
            "file://*/*"
        ]
    },
    "version": "0.9.0"
}
